pipeline {
    agent any
    tools {
        mvn "maven3"
    }
    stages {
        stage ("Git checkout") {
            steps{
                git credentialsId: 'git', url: 'https://github.com/kishore79/Simple-DevOps-Project'
            }
    }
stage('Build') {
            steps {
                sh "mvn clean install"
            }
        }
}
}
